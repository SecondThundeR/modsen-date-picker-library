{
  "name": "@secondthunder/modsen-date-picker-library",
  "version": "0.0.0",
  "description": "Date picker example library",
  "repository": {
    "type": "git",
    "url": "git+https://SecondThundeR@github.com/SecondThundeR/modsen-date-picker-library.git"
  },
